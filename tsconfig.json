{
    "compilerOptions": {
      "target": "ES2022",
      "module":"ES2022",
      "allowJs": true,
      "moduleResolution": "node",
      "outDir": "./out",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "noImplicitAny": true
    }
  }
